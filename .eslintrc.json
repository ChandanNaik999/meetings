{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "space-in-parens": ["error", "always"],
        "object-curly-newline": ["off"],
        "dot-notation": ["off"],
        "no-alert": ["off"],
        "no-undef": ["off"]
    }
}
